{
  "name": "api-schema-builder",
  "version": "1.0.4",
  "description": "build schema with validators for each endpoint",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "nyc": {
    "report-dir": "./coverage",
    "lines": 95,
    "statements": 95,
    "functions": 100,
    "branches": 90,
    "reporter": [
      "lcov",
      "text"
    ],
    "cache": true,
    "all": true
  },
  "license": "Apache-2.0",
  "scripts": {
    "test": "mocha --recursive",
    "test:coverage": "nyc npm test ",
    "coveralls": "cat ./coverage/lcov.info |./node_modules/.bin/coveralls",
    "lint": "./node_modules/.bin/eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zooz/api-schema-builder.git"
  },
  "keywords": [
    "ajv",
    "swagger",
    "OpenAPI",
    "open api",
    "validation",
    "validator"
  ],
  "bugs": {
    "url": "https://github.com/Zooz/api-schema-builder/issues"
  },
  "homepage": "https://github.com/Zooz/api-schema-builder.git",
  "author": "Gal Cohen",
  "dependencies": {
    "ajv": "^6.6.2",
    "clone-deep": "^4.0.1",
    "swagger-parser": "^6.0.2"
  },
  "devDependencies": {
    "ajv-keywords": "^3.4.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "request": "^2.88.0",
    "sinon": "^4.5.0",
    "uuid": "^3.3.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
